  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "file-type": "^18.2.1",
    "from2": "2.3.0",
    "pm2": "^5.2.2",
    "rclone": "^1.0.0",
    "stream-chunker": "1.2.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
