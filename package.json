{
  "type": "module",
  "name": "rclone-decrypt-express",
  "version": "1.0.0",
  "description": "A simple Express server for decrypting files from a specified URL using Rclone.",
  "main": "server.js",
  "scripts": {
    "restart": "pm2 restart server.js --no-daemon --watch --log",
